{	"manifest_version": 2,	"name": "Notifier",	"version": "1.1",	"description": "To see how message passing works",	"icons":{		"48": "icons/icon_48.png",		"96": "icons/icon_96.png"	},	"background":{		"scripts": ["js/bg.js"]	},	"browser_action":{		"default_icon": "icons/icon_48.png",		"default_title": "Notifier",		"default_popup": "index.html"	},	"content_scripts":[		{			"matches": ["<all_urls>"],			"js": ["js/cs.js"]		}	],	"page_action":{		"default_icon": "icons/icon_48.png",		"browser_style": true	},	"permissions": ["notifications"]}