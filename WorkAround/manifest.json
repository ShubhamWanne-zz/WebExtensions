{
	"manifest_version" : 2,
	"version": "1.1",
	"name": "WorkAround",
	"description": "To practise extensions",
	"icons":{
		"48": "icons/icon_48.png",
		"96": "icons/icon_96.png"
	},
	"permissions":[
		"nativeMessaging",
		"webRequest",
		"tabs"
	],
	"browser_action": {
		"default_icon": {
			"48": "icons/icon_48.png",
			"96": "icons/icon_96.png"
		},
		"default_title": "WorkAround",
		"default_popup": "html/popup.html"
	},
	"background": {
		"scripts": ["js/bg.js"],
		"persistent": false
	}
}